{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "https://raw.githubusercontent.com/Postmedia-Digital/postmedia-schema-content/develop/schema/0.1/content_elements/blockquote.json",
	"title": "A blockquote.",
    "description": "A blockquote content element.",
    "type": "object",
	"properties": {
		"id": {
			"description": "Incremental ID.",
			"type": "string"
		},
		"type": {
			"description": "Type of content element.",
			"type": "string",
			"enum": [ "blockquote" ],
			"default": "blockquote"
		},
		"content": {
			"description": "Content of the blockquote.",
			"type": "string"
		}
	},
	"required": [ "type", "content" ]
}
